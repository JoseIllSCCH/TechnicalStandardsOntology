        <terms:description>NIST Framework</terms:description>
    </owl:Class>
    


    <!-- http://scch/technical_standards#AuthenticationMechanism -->


    <owl:Class rdf:about="http://scch/technical_standards#AuthenticationMechanism">
        <rdfs:subClassOf rdf:resource="http://scch/technical_standards#Mechanism"/>
        <terms:description>Method used to prove the authenticity of an entity</terms:description>
        <terms:provenance>ETSI 303645</terms:provenance>
    </owl:Class>
    


    <!-- http://scch/technical_standards#BooleanConstraint -->


    <owl:Class rdf:about="http://scch/technical_standards#BooleanConstraint">
        <rdfs:subClassOf rdf:resource="http://scch/technical_standards#Constraint"/>
        <terms:description>Represents a boolean constraint also interpreted as condition.</terms:description>
    </owl:Class>
    


    <!-- http://scch/technical_standards#Buyer -->


    <owl:Class rdf:about="http://scch/technical_standards#Buyer">
