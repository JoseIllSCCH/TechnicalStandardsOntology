        <terms:provenance>ETSI 303645</terms:provenance>
    </owl:Class>
    


    <!-- http://scch/technical_standards#Actuator -->


    <owl:Class rdf:about="http://scch/technical_standards#Actuator">
        <rdfs:subClassOf rdf:resource="http://scch/technical_standards#Device"/>
