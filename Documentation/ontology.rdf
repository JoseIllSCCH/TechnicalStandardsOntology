    <owl:Class rdf:about="http://scch/technical_standards#Connection">
        <terms:description>Object that is used as communication channel between components. This connection can be attributed and configured.</terms:description>
    </owl:Class>
    


    <!-- http://scch/technical_standards#ConstrainedDevice -->


    <owl:Class rdf:about="http://scch/technical_standards#ConstrainedDevice">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://scch/technical_standards#Device"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://scch/technical_standards#hasDevicePhysicalLimitation"/>
                        <owl:someValuesFrom rdf:resource="http://scch/technical_standards#DevicePhysicalLimitation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <terms:description>Device which has physical limitations in either the ability to process data, the ability to communicate data, the ability to store data or the ability to interact with the user, due to restrictions that arise from its intended use</terms:description>
        <terms:provenance>ETSI 303645</terms:provenance>
    </owl:Class>
    


    <!-- http://scch/technical_standards#Constraint -->


    <owl:Class rdf:about="http://scch/technical_standards#Constraint">
