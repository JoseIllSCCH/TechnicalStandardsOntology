        <terms:description>Logical or physocal link between assets.</terms:description>
        <rdfs:comment>ETSI 303645</rdfs:comment>
    </owl:Class>
    


    <!-- http://scch/technical_standards#Component -->


    <owl:Class rdf:about="http://scch/technical_standards#Component">
