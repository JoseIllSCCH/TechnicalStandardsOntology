    </owl:Class>
    


    <!-- http://scch/technical_standards#ComponentSolution -->


    <owl:Class rdf:about="http://scch/technical_standards#ComponentSolution">
        <rdfs:subClassOf rdf:resource="http://scch/technical_standards#SolutionStructure"/>
    </owl:Class>
    


