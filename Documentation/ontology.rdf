    <owl:Class rdf:about="http://scch/technical_standards#APIKind">
        <rdfs:subClassOf rdf:resource="http://scch/technical_standards#Kind"/>
        <rdfs:comment>There are mulitple kinds such as REST, ROA, RMC, RMI</rdfs:comment>
    </owl:Class>
    


    <!-- http://scch/technical_standards#AccessControlMechanism -->


    <owl:Class rdf:about="http://scch/technical_standards#AccessControlMechanism">
        <rdfs:subClassOf rdf:resource="http://scch/technical_standards#Mechanism"/>
