        <terms:provenance>NIST Framework</terms:provenance>
    </owl:Class>
    


    <!-- http://scch/technical_standards#Capability -->


    <owl:Class rdf:about="http://scch/technical_standards#Capability">
        <owl:equivalentClass rdf:resource="http://scch/technical_standards#Functionality"/>
        <terms:description>Ability to perform a specified task</terms:description>
        <rdfs:comment>A delegated capability is not native to the component,however, the component is considered to posses it as it is inherited from a surrounding system component.</rdfs:comment>
    </owl:Class>
    


    <!-- http://scch/technical_standards#Category -->


    <owl:Class rdf:about="http://scch/technical_standards#Category">
        <terms:description>General concept to define possible categories and divivsions of elements.</terms:description>
    </owl:Class>
    


    <!-- http://scch/technical_standards#Channel -->


    <owl:Class rdf:about="http://scch/technical_standards#Channel">
