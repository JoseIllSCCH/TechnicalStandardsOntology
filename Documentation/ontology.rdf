        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:license rdf:resource="https://scch.org/technical_standards#CC_Attribution_4-0_International_License"/>
        <vann:preferredNamespacePrefix xml:lang="en">tsso</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri xml:lang="en">https://scch.org/technical_standards</vann:preferredNamespaceUri>
        <owl:versionInfo xml:lang="en">0.1</owl:versionInfo>
    </owl:Ontology>
    


