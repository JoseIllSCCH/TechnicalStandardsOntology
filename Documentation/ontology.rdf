        <terms:provenance>ETSI 303645</terms:provenance>
    </owl:Class>
    


    <!-- http://scch/technical_standards#Device -->


    <owl:Class rdf:about="http://scch/technical_standards#Device">
        <rdfs:subClassOf rdf:resource="http://scch/technical_standards#Product"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scch/technical_standards#isSW"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
            </owl:Restriction>
