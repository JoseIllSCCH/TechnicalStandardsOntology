###  https://scch.org/technical_standards#achieves
tsso:achieves rdf:type owl:ObjectProperty ;
              owl:inverseOf tsso:promotedBy ;
              rdfs:domain tsso:Outcome ;
              rdfs:range tsso:Driver ;
              dcterms:provenance "NIST Framework" ;
              rdfs:label "achieves"@en .


###  https://scch.org/technical_standards#addressedBy
tsso:addressedBy rdf:type owl:ObjectProperty ;
                 owl:inverseOf tsso:addresses ;
                 rdfs:label "addressed By"@en .


###  https://scch.org/technical_standards#addresses
tsso:addresses rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( tsso:Capability
                                           tsso:CounterMeasure
                                           tsso:Provision
                                         )
                           ] ;
               rdfs:range tsso:Threat ;
               rdfs:label "addresses"@en .


###  https://scch.org/technical_standards#addressesConcern
tsso:addressesConcern rdf:type owl:ObjectProperty ;
                      rdfs:range tsso:Concern ;
                      dcterms:provenance "NIST Framework" ;
                      rdfs:label "addresses Concern"@en .


