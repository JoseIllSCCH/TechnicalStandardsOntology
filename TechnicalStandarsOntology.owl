                                        dcterms:creator "Gandalf Denk"@en ,
                                                        "Georg Buchgeher"@en ,
                                                        "Jose Illescas"@en ,
                                                        "Lisa Ehrlinger"@en ,
                                                        "Peter Panholzer"@en ,
                                                        "Thomas Brandstetter"@en ;
                                        dcterms:license <https://creativecommons.org/licenses/by/4.0/> ,
                                                        tsso:CC_Attribution_4-0_International_License ;
